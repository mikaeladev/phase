{
  "private": true,
  "name": "@plugin/music",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@distube/youtube": "^1.0.4",
    "@distube/ytdl-core": "^4.16.8",
    "@phasejs/core": "workspace:*"
  },
  "devDependencies": {
    "@plugin/voice": "workspace:*",
    "distube": "^5.0.7"
  },
  "peerDependencies": {
    "@discordjs/voice": "^0.18.0",
    "discord.js": "^14.18.0"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "minify": true,
    "splitting": true,
    "sourcemap": true,
    "format": "esm",
    "entry": [
      "./src/index.ts"
    ]
  }
}
